@{
    ViewBag.Title = "Home Page";
}
<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
</head>
<body>
    <div class="container-fluid">
        <div class="row" align="center">
            <div class="col-md-12">
                <h2 align="center">About Code the Way</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <h4 align="center">Mission</h4>

                <p align="center">
                    The mission of code the way is to facilitate the collaboration of schools, companies and nonprofits to offer students advanced computer science training and real world job experiences working on projects that benefit nonprofits, increase certified computer science teachers, and contribute to the technology talent base.
                </p>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <h4 align="center">Vision</h4>
                <p align="center">
                    High school students in Greater Milwaukee will gain access to high quality computer science education and real-world job experiences, positioning them for success in technology-related fields.
                </p>
            </div>
        </div>
    </div>
</body>
</html>
